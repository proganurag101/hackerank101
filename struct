#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include<string>
#include <algorithm>
using namespace std;
 /*                                                                             SOLUTION
                                                                                START                                                                                           */
struct student{
    int age;
    string first_name; 
    string last_name;
    int standard;
};

                                                                               //END
int main() {
    student st;
    
    cin >> st.age >> st.first_name >> st.last_name >> st.standard;
    cout << st.age << " " << st.first_name << " " << st.last_name << " " << st.standard;
    
    return 0;
}
